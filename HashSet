package demo03_Set;
import java.util.HashSet;
import java.util.Set;

public class HashSetDemo {

	public static void main(String[] args) {
		
		
		Set<Double>	temp=new HashSet<Double>();
		temp.add(36.5);
		temp.add(40.5);
		temp.add(42.5);
		temp.add(39.5);
		temp.add(42.5);//Duplicates are n't allowed,
		temp.add(38.5);//Set- sequence isn't there,	
		temp.add(null); //Null is allowed in HashSet but not in TreeSet
		System.out.println(temp);
		
		if(temp.contains(39.5));{// checking
			temp.remove(39.5);// Remove element
		}
		System.out.println(temp);
		
	}

}
